{
  "name": "@selene/core",
  "version": "3.0.0",
  "description": "Selene Song Core - The Lunar Song Backend",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx index.ts",
    "nuclear": "npm run build && npm run start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:load": "npx ts-node Apollo/Testing/runLoadTests.ts",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write .",
    "cluster:start": "npm run build && pm2 start ecosystem.config.cjs",
    "cluster:stop": "pm2 stop selene-cluster",
    "cluster:restart": "pm2 restart selene-cluster",
    "cluster:delete": "pm2 delete selene-cluster",
    "cluster:logs": "pm2 logs selene-cluster",
    "cluster:monit": "pm2 monit",
    "cluster:status": "pm2 status",
    "quick:check": "node quick-check.js",
    "stress:test": "node disciplina-romana.js",
    "crisol:launch": "npm run build && node apollo-crisol-launcher.js",
    "crisol:full": "npm run cluster:start && npm run quick:check",
    "crisol:discipline": "npm run quick:check && npm run stress:test",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "audit:eslint": "eslint . --ext .ts --format=json | jq length",
    "clean:lint": "eslint . --ext .ts --fix --quiet"
  },
  "keywords": [
    "selene",
    "song",
    "core",
    "backend",
    "musical",
    "consensus",
    "distributed",
    "lunar"
  ],
  "author": "PunkClaude & RaulVisionario",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/express4": "^1.1.2",
    "@types/body-parser": "^1.19.6",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^2.2.0",
    "bull": "^4.12.2",
    "circom2": "^0.2.22",
    "circomlib": "^2.0.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "graphql": "^16.11.0",
    "graphql-subscriptions": "^3.0.0",
    "graphql-ws": "^6.0.6",
    "helmet": "^7.1.0",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^3.0.5",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "midi-writer-js": "^3.1.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "node-schedule": "^2.1.1",
    "pg": "^8.16.3",
    "redis": "^4.7.1",
    "snarkjs": "^0.7.5",
    "socket.io": "^4.8.1",
    "subscriptions-transport-ws": "^0.11.0",
    "typescript": "^5.9.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^4.10.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.13",
    "@types/node-cron": "^3.0.11",
    "@types/node-schedule": "^2.1.8",
    "@types/pg": "^8.10.9",
    "@types/redis": "^4.0.11",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^4.0.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "ioredis-mock": "^8.13.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "ts-morph": "^27.0.0",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6",
    "vitest": "^4.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pinkyfloyder/Dentiagest.git"
  },
  "bugs": {
    "url": "https://github.com/pinkyfloyder/Dentiagest/issues"
  },
  "homepage": "https://github.com/pinkyfloyder/Dentiagest#readme"
}
